
<h3><%= @page.name %></h3>

<ul class="page_show">
  <% @page.webclips.each do |webclip| %>
      <li>
        <div>
          <%= image_base_64_tag(webclip.webclip_icon)%>
          <h5><%= webclip.name %></h5>
        </div>
        <span><%= link_to 'Edit', edit_admin_webclip_path(webclip) %>  |  <%= link_to "X", remove_webclip_admin_page_path(@page, webclip), data: { confirm: 'Are you sure?' } %></span>
      </li>
      
  <% end %>
</ul>
<br/><br/>
<%= link_to 'Edit Page', edit_admin_page_path(@page) %> |
<%= link_to 'Back', admin_pages_path %>
